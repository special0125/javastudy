package ex02_2d_array;
/*
 * 2. 구구단의 결과만 2차원 배열에 저장하고 출력하기
 * 2 4 6 ... 18
 * 3 6 8 ... 27
 * ...
 * 9 18 27 ...
 */
public class Quiz02 {
	public static void main(String[] args) {
		
		int[][] a = new int[9][9];
		
		for(int i = 2; i < 9; i++) {
			for(int j = 1; j < 9; j++) {
				System.out.print(i + "x" + j +"=" + (i*j));
			}
			System.out.println();
		}
		
		
	}
}
