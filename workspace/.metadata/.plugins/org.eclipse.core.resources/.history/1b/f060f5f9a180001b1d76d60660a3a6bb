package ex02_2d_array;
/*
 * 2. 구구단의 결과만 2차원 배열에 저장하고 출력하기
 * 2 4 6 ... 18
 * 3 6 8 ... 27
 * ...
 * 9 18 27 ...
 */
public class Quiz02 {
	public static void main(String[] args) {
		
		int[][] values = new int[9][9];
		
		for(int i = 2; i < 9; i++) {
			for(int j = 1; j < 9; j++) {
				a[i-2][j-1] = i*j;
			}
		}
		
	
		for(int[] a : values) {
			for(int b : a) {
				System.out.println(a);
			}
		}
	}
}
