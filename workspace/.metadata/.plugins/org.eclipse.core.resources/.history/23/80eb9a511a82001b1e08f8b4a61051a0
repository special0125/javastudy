package ex02_method;

public class Mainclass {
	public static void main(String[] args) {
		
		Person p = new Person();
		p.set("alice", 30, '여', false);	// 인수가 4개
		System.out.println(p.name);
		System.out.println(p.age);
		System.out.println(p.gender);
		System.out.println(p.isKorean);
		
		p.info();
	}
}
