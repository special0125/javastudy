package quiz02;

public class Circle {
	
	// field
	private Coord center;
	private double radius;
	
	// constructor
	public Circle(Coord center, double radius) {
		this.center = center;
		this.radius = radius;
	}
	
	// method
	public void info() {
		
		double circlearea = Math.PI * Math.pow(this.radius, 2);
		System.out.println(center);
	}
	
	
	
}

